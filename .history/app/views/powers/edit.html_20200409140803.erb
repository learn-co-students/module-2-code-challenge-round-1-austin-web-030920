<%= form_for @power do |f|%>
<%= f.label :name %>
<%= f.text_field :name %>
<br><br>
<%= f.label :description %>
<%= f.text_field :description %>
<br><br>
<%= f.collection_check_boxes :heroine_ids, Heroine.all, :id, :name do |b| %>
    <%= b.label { b.check_box } %>
<% end %>

<br>
<%= f.submit %>
<% end %>