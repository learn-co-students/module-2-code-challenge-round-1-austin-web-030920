<h1>New Heroine Form</h1>

<% if @errors %>

<h3><%= pluralize(@errors.count, "error") %> occured:</h3>
<ul>
<% @errors.full_messages.each do |message| %>
<li><%= message %></li>
<% end %>
</ul>

<% end %>


<%= form_for @heroine do |f| %>

<%= f.label :super_name %>
<%= f.text_field :super_name%><br />

<%= f.label :name %>
<%= f.text_field :name%><br />

<%= f.label "Choose Power" %>
<%= f.collection_select :power, Power.all, :id, :name %><br />

<%= f.submit %>
<% end %>