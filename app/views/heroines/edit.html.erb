<h3><%= form_for (@heroine) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>
    <%= f.label :super_name %>
    <%= f.text_field :super_name %><br><br>
    <%= f.collection_select(:power_id, Power.all, :id, :name, prompt: true) %>
    <br><br>
    <%= f.submit %>
    <% end %></h3>